{
    "name": "slim/slim-skeleton",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/soatok/headless-lounge-bot",
    "license": "ISC",
    "authors": [
        {
            "name": "Soatok",
            "email": "soatok.dhole@gmail.com",
            "homepage": "https://soatok.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "slim/slim": "^3.1",
        "soatok/anthrokit": "^0|^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "vimeo/psalm": "^3"
    },
    "autoload": {
        "psr-4": {
            "Soatok\\HeadlessLoungeBot\\": "src/HeadlessLoungeBot"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Soatok\\HeadlessLoungeBot\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S 127.0.0.1:8080 -t public",
        "test": "phpunit"
    }
}
